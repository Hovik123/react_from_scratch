{
  "name": "react_from_scratch",
  "version": "1.0.0",
  "description": "React + TypeScript + Webpack boilerplate",
  "main": "src/main.tsx",
  "repository": "git@github.com:nitishdayal/react_from_scratch.git",
  "author": "Nitish Dayal <nitishdayal@outlook.com>",
  "license": "MIT",
  "scripts": {
    "predist": "rm -rf dist",
    "dist": "webpack --config config/webpack/webpack.config.prod.js",
    "start": "webpack-dashboard -- webpack-dev-server --config config/webpack/webpack.config.dev.js",
    "predist:preact": "rm -rf dist",
    "dist:preact": "webpack --config config/webpack/preact/webpack.config.preact.prod.js",
    "start:preact": "webpack-dev-server --config config/webpack/preact/webpack.config.preact.prod.js"
  },
  "dependencies": {
    "polished": "^1.0.3",
    "preact": "^8.1.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "styled-components": "^1.4.5"
  },
  "devDependencies": {
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.22",
    "@types/react-dom": "^15.5.0",
    "@types/react-router-dom": "^4.0.4",
    "@types/webpack-env": "^1.13.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "preact-compat": "^3.15.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "source-map-loader": "^0.2.1",
    "ts-loader": "^2.0.3",
    "typescript": "next",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.4.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.4",
    "webpack-merge": "^4.1.0"
  }
}