{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "diagnostics": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "react",
      "react-dom",
      "node",
      "webpack-env",
      "react-router-dom"
    ]
  },
  "exclude": [
    "./node_modules"
  ]
}